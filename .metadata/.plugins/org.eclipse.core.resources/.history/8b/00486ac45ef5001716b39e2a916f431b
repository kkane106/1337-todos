angular.module('appModule', ['ngRoute'])
.config(function($routeProvider){
	$routeProvider
		.when('/', {
			template : '<home></home>'
		})
		.when('/about', {
			template : '<about></about>'
		})
		.when('/todo', {
			template : '<todo-list></todo-list>'
		})
		.otherwise({
			template : '<h1>NOT FOUND</h1>'
		})
})		